{
	"name": "react-kit",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"format": "prettier --write ./src",
		"preview": "vite preview",
		"serve": "json-server -w server/db.json -p 7000 ",
		"start": "concurrently \"npm run serve\" \"npm run dev\"",
		"prepare": "husky"
	},
	"dependencies": {
		"@ant-design/icons": "^5.6.1",
		"@reduxjs/toolkit": "^2.6.1",
		"antd": "^5.24.4",
		"react": "^18",
		"react-dom": "^18",
		"react-redux": "^9.2.0",
		"react-router": "^7.3.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"@types/node": "^22.13.10",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@vitejs/plugin-react-swc": "^3.8.0",
		"concurrently": "^9.1.2",
		"eslint": "^9.21.0",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^15.15.0",
		"husky": "^9.1.7",
		"json-server": "1.0.0-beta.3",
		"prettier": "^3.5.3",
		"sass": "^1.86.0",
		"typescript": "~5.8.2",
		"typescript-eslint": "^8.24.1",
		"vite": "^6.2.0"
	}
}
